{
  "name": "repo",
  "version": "1.0.0",
  "description": "Static Gatsby site with customised Wordpress CMS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "init:client": "cd client && npm i",
    "init:wordpress": "cd wordpress && npm i",
    "init:both": "npm run init:client && npm run init:wordpress",
    "dev:client": "cd client && gatsby develop",
    "dev:wordpress": "cd wordpress && npm run dev:start",
    "dev:start": "npm run dev:wordpress && sleep 20 && npm run dev:client",
    "dev:stop": "cd wordpress && npm run dev:stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arrested-developer/neu-hackney.git"
  },
