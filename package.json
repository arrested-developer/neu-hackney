{
  "name": "repo",
  "version": "1.0.0",
  "description": "Static Gatsby site with customised Wordpress CMS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "init:client": "cd client && npm i",
    "init:wordpress": "cd backend && npm i",
    "init:both": "npm run init:client && npm run init:wordpress",
    "dev:client": "cd client && npm run develop",
    "dev:backend": "cd backend && npm run dev:start",
    "dev:start": "npm run dev:backend && sleep 20 && npm run dev:client",
    "dev:stop": "cd backend && npm run dev:stop"
  },
